{% if isPaginated %}   
    <div class="row">
        <nav aria-label="Page navigation example">
            <ul class="pagination">
                {% if page != 1 %}
                    <li class="page-item">
                        <a 
                            class="page-link link-success" 
                            href="{{path('app_partner_list', {page: page - 1, nbre: nbre})}}"
                        >
                            Previous
                        </a>
                    </li>
                {% endif %}
                {% for i in range(1, nbrePage) %}
                    <li class="page-item">
                        <a 
                            class="page-link link-success" 
                            href="{{path('app_partner_list', {page: i, nbre: nbre})}}"
                        >
                            {{ i }}
                        </a>
                    </li>
                {% endfor %}
                {% if page != nbrePage %}
                    <li class="page-item">
                        <a 
                            class="page-link link-success" 
                            href="{{path('app_partner_list', {page: page + 1, nbre: nbre})}}"
                        >
                            Next
                        </a>
                    </li>
                {% endif %}
            </ul>
        </nav>
    </div>
{% endif %}